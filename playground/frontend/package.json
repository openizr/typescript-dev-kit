{
  "license": "MIT",
  "devDependencies": {
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/vue": "^6.5.1",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "typescript-dev-kit": "file:../../../../typescript-dev-kit/dist"
  },
  "dependencies": {
    "basx": "^1.3.7",
    "diox": "^4.0.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "svelte": "^3.46.4",
    "vue": "^3.2.31"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/typescript-dev-kit/main.js"
    ]
  },
  "tsDevKitConfig": {
    "target": "web",
    "devServer": {
      "host": "0.0.0.0",
      "port": 5001
    },
    "html": "./html/index.html",
    "srcPath": "src",
    "distPath": "public",
    "banner": "/*!\n * Copyright (c) Matthieu Jabbour.\n * All rights reserved.\n */",
    "env": {
      "development": {
        "ENV": "development",
        "NODE_ENV": "development"
      },
      "production": {
        "ENV": "production",
        "NODE_ENV": "production"
      }
    }
  },
  "scripts": {
    "init": "node_modules/typescript-dev-kit/scripts/init.sh",
    "dev": "cd node_modules/typescript-dev-kit/ && node scripts/dev.js",
    "build": "cd node_modules/typescript-dev-kit/ && node scripts/build.js",
    "check": "cd node_modules/typescript-dev-kit/ && node scripts/check.js",
    "test": "cd node_modules/typescript-dev-kit/ && node scripts/test.js",
    "doc": "typedoc --out ./docs/ --exclude \"**/__+(tests|mocks)__/**\" src/scripts/javascript.jsx",
    "postinstall": "rm -f node_modules/.eslintcache"
  }
}
